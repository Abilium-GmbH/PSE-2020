<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="report_overview">
        <t t-call="web.html_container">
            <div class="page">
                <table style="border: 2px solid black;">
                    <tr style="font-weight:bold;">
                        <th>
                            Employee
                        </th>
                        <th>
                            Project
                        </th>
                        <t t-foreach="docs" t-as="o">
                            <th>
                                <span t-field="o.week_string"/>
                            </th>
                        </t>
                    </tr>
                    <t t-foreach="docs" t-as="doc">
                        <tr>
                            <td>
                                <span t-field="doc.employee"/>
                            </td>
                             <td>
                                    <span t-field="doc.project"/>
                                </td>
                            <div t-if="doc.end_date>=doc.today">

                                <td>
                                    <span t-field="doc.weekly_workload"/>
                                </td>

                            </div>


                        </tr>
                    </t>
                </table>
            </div>
        </t>

    </template>
</odoo>